package rrs.vo;

import java.util.List;

public class ReservationDetailVO {
	public String rs_id; // 예약 번호
	public String mem_name; // 회원 이름
	public String rs_time; // 예약 일자
	public String rs_datetime; // 방문 날짜
	public int rs_people; // 방문 인원
	public String rst_name; // 가게 이름
	public String rst_tel; // 가게 연락처
	public String rst_addr; // 가게 주소
	public int rm_qty; // 주문 수량

	public List<RsMenuVO> menu_list; // 예약 메뉴 리스트

	public String getRs_id() {
		return rs_id;
	}

	public void setRs_id(String rs_id) {
		this.rs_id = rs_id;
	}

	public String getMem_name() {
		return mem_name;
	}

	public void setMem_name(String mem_name) {
		this.mem_name = mem_name;
	}

	public String getRs_time() {
		return rs_time;
	}

	public void setRs_time(String rs_time) {
		this.rs_time = rs_time;
	}

	public String getRs_datetime() {
		return rs_datetime;
	}

	public void setRs_datetime(String rs_datetime) {
		this.rs_datetime = rs_datetime;
	}

	public int getRs_people() {
		return rs_people;
	}

	public void setRs_people(int rs_people) {
		this.rs_people = rs_people;
	}

	public String getRst_name() {
		return rst_name;
	}

	public void setRst_name(String rst_name) {
		this.rst_name = rst_name;
	}

	public String getRst_tel() {
		return rst_tel;
	}

	public void setRst_tel(String rst_tel) {
		this.rst_tel = rst_tel;
	}

	public String getRst_addr() {
		return rst_addr;
	}

	public void setRst_addr(String rst_addr) {
		this.rst_addr = rst_addr;
	}

	public List<RsMenuVO> getMenu_list() {
		return menu_list;
	}

	public void setMenu_list(List<RsMenuVO> menu_list) {
		this.menu_list = menu_list;
	}

	public int getRm_qty() {
		return rm_qty;
	}

	public void setRm_qty(int rm_qty) {
		this.rm_qty = rm_qty;
	}

}
